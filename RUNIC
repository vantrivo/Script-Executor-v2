	Name = "become knockoff flash and catch the rainbow",
	Callback = function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/vantrivo/Script-Executor-v2/refs/heads/main/goofy%20button'))()
	end,
})
local TOOL = Main:CreateButton({
	Name = "tools of doom and despair",
	Callback = function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/vantrivo/Script-Executor-v2/refs/heads/main/tools'))()
	end,
})
local invis = Trolling:CreateButton({
	Name = "W-Whos there!!?!?? --alberts victim",
	Callback = function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/vantrivo/Script-Executor-v2/refs/heads/main/invisibility'))()
		Rayfield:Notify({
			Title = "hey stupid guess what",
			Content = "re-execute when u die or else u dont be invisible",
			Duration = 4,
			Image = 4483362458,
		})
		
	end,
})

local Cred = Creds:CreateSection("trivo made ts overnight til 3:12AM 7/1/2025")


